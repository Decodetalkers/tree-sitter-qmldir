{
  "name": "qmldir",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "line"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "line"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "left",
              "content": {
                "type": "SYMBOL",
                "name": "varname"
              }
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "varname"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "spliter"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "comment"
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "varname": {
      "type": "PATTERN",
      "value": "[\\dA-Za-z\\-_]+"
    },
    "spliter": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "STRING",
          "value": ":"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]+"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

